{
  "name": "communi-design-system",
  "description": "design system for CommuniDAO",
  "version": "0.0.14",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/CommuniDAO/communi-design-system"
  },
  "author": "Julian Clatro <hola@julianclatro.com> (https://hacksur.com)",
  "contributors": [
    "Julian Clatro <hola@julianclatro.com> (https://hacksur.com)"
  ],
  "license": "MIT",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "xo",
    "lint:md": "remark . -qfo",
    "pretest": "npm run lint",
    "clean": "rm -rf lib",
    "build": "tsc",
    "webpack": "webpack --mode production",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "cross-env NODE_ENV=production build-storybook",
    "tailwind:dev": "cross-env NODE_ENV=development tailwindcss build -i styles/main.css -o styles/index.css -c tailwind.config.js",
    "tailwind:build": "cross-env NODE_ENV=production tailwindcss build -i styles/main.css -o styles/index.css -c tailwind.config.js",
    "watch-tailwind": "npx tailwindcss -i styles/main.css -o styles/index.css -c tailwind.config.js --watch",
    "watch-build": "tsc -w",
    "watch-storybook": "start-storybook --ci -p 6006",
    "dev": "concurrently 'npm:watch-*'",
    "production": "npm run clean && npm run tailwind:build && npm run build"
  },
  "keywords": [
    "communi-design-system",
    "lass"
  ],
  "dependencies": {
    "@h6s/calendar": "latest",
    "@headlessui/react": "latest",
    "@headlessui-float/react": "^0.8.0",
    "luxon": "^3.3.0",
    "rci": "latest",
    "framer-motion": "^6.5.1",
    "date-fns": "latest",
    "ahooks": "latest",
    "use-is-focused": "latest",
    "dayzed": "latest"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-essentials": "6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/builder-webpack5": "6.5.14",
    "@storybook/manager-webpack5": "6.5.14",
    "@storybook/react": "6.4.14 ",
    "@tailwindcss/aspect-ratio": "latest",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/line-clamp": "latest",
    "@tailwindcss/typography": "latest",
    "@types/luxon": "latest",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "latest",
    "babel-loader": "latest",
    "clsx": "latest",
    "codecov": "latest",
    "concurrently": "latest",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "dotenv": "latest",
    "eslint": "latest",
    "eslint-config-xo-lass": "latest",
    "eslint-config-xo-react": "latest",
    "eslint-config-xo-typescript": "latest",
    "fixpack": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "lunarphase-js": "latest",
    "nyc": "latest",
    "pm2": "latest",
    "postcss": "latest",
    "qrcode-generator": "1.4.1",
    "react": "^17",
    "react-dom": "^17",
    "react-use": "latest",
    "remark-cli": "^9.0.0",
    "remark-preset-github": "latest",
    "storybook-addon-designs": "latest",
    "storybook-dark-mode": "latest",
    "tailwindcss": "^3.2.4",
    "ts-loader": "latest",
    "typescript": "^4.9.3",
    "webpack": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest",
    "xo": "latest"
  },
  "peerDependencies": {
    "clsx": "^1.2.1",
    "react": "^17",
    "react-dom": "^17"
  },
  "engines": {
    "node": ">= 10"
  },
  "bugs": {
    "url": "https://github.com/CommuniDAO/communi-design-system/issues",
    "email": "hola@julianclatro.com"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass",
      "xo-react",
      "xo-typescript"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "webpack": true
  },
  "homepage": "https://github.com/CommuniDAO/communi-design-system",
  "volta": {
    "node": "16.15.1"
  }
}
